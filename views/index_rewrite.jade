                  //$('#'+temp_id).attr({'lastitem':msg.lastitem,'nick':msg.mtext});
                  arr[index]= temp_friend;
                  }
                  index++;
                  if(index<arr.length){
                    callback(arr,index,callback);
                  }
                  });
             }
