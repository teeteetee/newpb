           if(!global_items_store[$(el).attr('teamlist_id')].itemstore[$(el).attr('index')].item_comment||global_items_store[$(el).attr('teamlist_id')].itemstore[$(el).attr('index')].item_comment=='--'){
             $('.item_comment').css('display','none');
