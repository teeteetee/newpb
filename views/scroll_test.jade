doctype html(lang='en')
head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1') 
    title scroll
    link(href='/bootstrap/css/bootstrap.min.css', rel='stylesheet')
    script(src='https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js')       
    script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
  body( data-spy="scroll"  style='position:relative;padding-top:51px;')
    nav(class="navbar navbar-default navbar-fixed-top")
      .container
        .navbar-header
          button(type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar")
            span(class="sr-only") Toggle navigation
            span(class="icon-bar")
            span(class="icon-bar")
            span(class="icon-bar")
          a.navbar-brand( href="#") Project name
        #navbar(class="navbar-collapse collapse")
          ul.nav.navbar-nav
            li.active
              a(href="#first") First
            li
              a(href="#second") Second
            li
              a(href="#third") Third
            li
              a(href="#fourth") Fourth
            li
              a(href="#fifth") fifth 
            //
              li.dropdown
                a(href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false") Dropdown span class="caret"
                ul(class="dropdown-menu")
                  li
                    a(href="#") Action
                  li
                    a(href="#") Another action
                  li
                    a(href="#") Something else here
                  li(role="separator" class="divider")
                  li(class="dropdown-header") Nav header
                  li
                    a(href="#") Separated link
                  li
                    a(href="#") One more separated link
    .container
      .row#first(style='height:500px;background-color:#ddd;')
        .col-xs-12
          h1 First row
      .row#second(style='height:500px;background-color:#ffc;')
        .col-xs-12
          h1 Second row
      .row#third(style='height:500px;background-color:cyan;')
        .col-xs-12
          h1 Third row
      .row#fourth(style='height:500px;background-color:green;')
        .col-xs-12
          h1 fourth row
      .row#fifth(style='height:500px;background-color:green;')
        .col-xs-12
          h1 fifth row
       

    
